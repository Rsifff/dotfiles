! urxvt ----------------------------------------------------------------
URxvt.font: xft:Hack Nerd Font Mono:style=Regular:size=13
URxvt.boldFont: xft:Hack Nerd Font Mono:style=Bold:size=13
URxvt*scrollBar: false
URxvt*foreground: #f2f2f2
URxvt*transparent: true
URxvt*shading: 60
URxvt.perl-lib: ~/.urxvt/ext 
URxvt.perl-ext-common:  default,matcher,selection-to-clipboard,paste_clipboard,resize-font
URxvt.url-launcher:     /usr/bin/firefox
URxvt.matcher.button:   1
URxvt.keysym.Control-minus:   resize-font:smaller
URxvt.keysym.Control-plus:    resize-font:bigger
URxvt.keysym.Control-equal:   resize-font:reset
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
